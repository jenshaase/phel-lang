(ns phel\test\core\more-sequence-operation
  (:require phel\test :refer [deftest is-equals]))

(deftest test-tree-seq
  (is-equals @[[[1 2 [3]] [4]]
               [1 2 [3]]
               1
               2
               [3]
               3
               [4]
               4] (tree-seq indexed? identity [[1 2 [3]] [4]]) "tree-seq"))

(deftest test-flatten
  (is-equals @[1 2 3 4 5 6 7] (flatten [1 [2 [3 4 [5]] 6 [7]]]) "flatten")
  (is-equals @[] (flatten nil) "flatten nil"))
