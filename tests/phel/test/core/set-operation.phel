(ns phel\test\core\set-operation
  (:require phel\test :refer [deftest is-equals]))

(deftest test-set-union
  (is-equals (set) (union) "set 0-ary union")
  (is-equals (set 0 1 2 3) (union (set 1 2) (set 0 3)) "set union")
  (is-equals (set 0 1 2 3) (union (set 1 2) (set 0 1 2 3)) "set union with common values"))

(deftest test-intersection
  (is-equals (set) (intersection (set 1 2) (set 0 3)) "set intersection")
  (is-equals (set 1 2) (intersection (set 1 2) (set 0 1 2 3)) "set intersection with common values"))

(deftest test-difference
  (is-equals (set 1 2) (difference (set 1 2) (set 0 3)) "set difference")
  (is-equals (set) (difference (set 1 2) (set 0 1 2 3)) "set difference")
  (is-equals (set 0 3) (difference (set 0 1 2 3) (set 1 2)) "set difference with common values"))

(deftest test-symmetric-difference
  (is-equals (set 0 1 2 3) (symmetric-difference (set 1 2) (set 0 3)) "set symmetric-difference")
  (is-equals (set 0 3) (symmetric-difference (set 1 2) (set 0 1 2 3)) "set symmetric-difference with common values"))
